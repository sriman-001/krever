<sdf version="1.6">
  <model name="Krever">
    <static>false</static>
    
    <link name="base_link">
      <inertial>
        <mass>1.0</mass> <!-- Adjust mass -->
        <inertia>
          <ixx>0.1</ixx> <!-- Adjust inertia -->
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy>
          <iyz>0.0</iyz>
          <izz>0.1</izz>
        </inertia>
      </inertial>
      <visual>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.2</length>
          </cylinder>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.2</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <joint name="base_to_left_wheel" type="revolute">
      <parent>base_link</parent>
      <child>left_wheel</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-10</lower>
          <upper>10</upper>
          <effort>100</effort>
          <velocity>10</velocity>
        </limit>
      </axis>
    </joint>

    <link name="left_wheel">
      <inertial>
        <mass>0.5</mass> <!-- Adjust mass -->
        <inertia>
          <ixx>0.01</ixx> <!-- Adjust inertia -->
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.01</iyy>
          <iyz>0.0</iyz>
          <izz>0.01</izz>
        </inertia>
      </inertial>
      <visual>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <cylinder>
            <radius>0.1</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- Add more links and joints as needed -->

  </model>
</sdf>
